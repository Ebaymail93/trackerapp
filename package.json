{
  "name": "gps-tracker-raspberry",
  "version": "1.0.0",
  "description": "GPS Tracker System for Raspberry Pi deployment",
  "main": "dist-server/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "preinstall": "node --version",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build --config vite.config.production.js",
    "build:backend": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist-server/index.js",
    "build:production": "npm run build:frontend && npm run build:backend",
    "start": "node dist-server/index.js",
    "dev": "npx tsx server/index.ts",
    "deploy": "./scripts/deploy-raspberry.sh",
    "db:init": "npx tsx server/db-init.ts",
    "db:migrate": "npx drizzle-kit push",
    "backup": "docker-compose -f docker-compose.raspberry.yml --env-file .env.raspberry exec postgres pg_dump -U gps_user gps_tracker > backups/backup-$(date +%Y%m%d-%H%M%S).sql",
    "logs": "docker-compose -f docker-compose.raspberry.yml --env-file .env.raspberry logs -f",
    "check-node": "node -e \"const v=process.version.slice(1).split('.'); if(+v[0]<18) { console.error('❌ Node.js 18+ required, current:', process.version); process.exit(1); } else { console.log('✅ Node.js version OK:', process.version); }\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@neondatabase/serverless": "^0.10.3",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.62.3",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "connect-pg-simple": "^10.0.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "framer-motion": "^11.13.1",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.4.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^6.30.0",
    "sonner": "^1.7.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.0",
    "wouter": "^3.3.5",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.9.0",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.28.1",
    "esbuild": "^0.24.0",
    "nanoid": "^5.0.9",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6"
  },
  "keywords": [
    "gps",
    "tracker",
    "raspberry-pi",
    "iot",
    "lilygo",
    "sim7000g"
  ],
  "author": "GPS Tracker Team",
  "license": "MIT"
}