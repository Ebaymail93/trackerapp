{
  "name": "gps-tracker-raspberry",
  "version": "1.0.0",
  "description": "GPS Tracker System for Raspberry Pi deployment",
  "main": "dist-server/index.js",
  "type": "module",
  "scripts": {
    "build": "vite build --config vite.config.production.js && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist-server/index.js",
    "start": "node dist-server/index.js",
    "deploy": "./scripts/deploy-raspberry.sh",
    "db:init": "npx tsx server/db-init.ts",
    "db:migrate": "npx drizzle-kit push",
    "backup": "docker-compose -f docker-compose.raspberry.yml --env-file .env.raspberry exec postgres pg_dump -U gps_user gps_tracker > backups/backup-$(date +%Y%m%d-%H%M%S).sql",
    "logs": "docker-compose -f docker-compose.raspberry.yml --env-file .env.raspberry logs -f"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.3",
    "bcrypt": "^5.1.1",
    "connect-pg-simple": "^10.0.0",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.5.1",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.462.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "recharts": "^2.13.3",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.3.5",
    "@tanstack/react-query": "^5.62.3",
    "framer-motion": "^11.13.1",
    "vaul": "^1.1.1",
    "next-themes": "^0.4.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.30.6",
    "esbuild": "^0.24.0",
    "postcss": "^8.5.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.15"
  },
  "keywords": [
    "gps",
    "tracker",
    "raspberry-pi",
    "iot",
    "lilygo",
    "sim7000g"
  ],
  "author": "GPS Tracker Team",
  "license": "MIT"
}